<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.select :title, ["Mr", "Mrs", "Miss"] %>
  </div>
  <div class="field">
    <%= f.label :"First Name" %><br />
    <%= f.text_field :firstName %>
  </div>
  <div class="field">
    <%= f.label :"Last Name" %><br />
    <%= f.text_field :lastName %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :"Phone Number" %><br />
    <%= f.text_field :phoneNum %>
  </div>
  <div class="field">
    <%= f.label :"Date of Birth" %><br />
    <%= f.date_select :dob %>
  </div>
  <div class="field">
	<%= f.label :username %><br />
	<%= f.text_field :username %>
  </div>
  <div class="field">
    <%= f.label :password %><br />
    <%= f.password_field :password_digest %>
  </div>
  
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
